<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/satteRabatteFragment">
    <fragment
        android:id="@+id/satteRabatteFragment"
        android:name="de.syntaxinstitut.myapplication.ui.fragmenteXml.AngeboteFragment"
        android:label="AngeboteFragment" />
    <fragment
        android:id="@+id/kategorienFragment"
        android:name="de.syntaxinstitut.myapplication.ui.fragmenteXml.KategorienFragment"
        android:label="KategorienFragment" >
        <action
            android:id="@+id/action_kategorienFragment_to_kategorienDetailFragment"
            app:destination="@id/kategorienDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/warenkorbFragment"
        android:name="de.syntaxinstitut.myapplication.ui.fragmenteXml.WarenkorbFragment"
        android:label="WarenkorbFragment" />
    <fragment
        android:id="@+id/auftragFragment"
        android:name="de.syntaxinstitut.myapplication.ui.fragmenteXml.AuftragFragment"
        android:label="AuftragFragment" />
    <fragment
        android:id="@+id/mehrFragment"
        android:name="de.syntaxinstitut.myapplication.ui.fragmenteXml.MehrFragment"
        android:label="MehrFragment" />
    <fragment
        android:id="@+id/kategorienDetailFragment"
        android:name="de.syntaxinstitut.myapplication.ui.fragmenteXml.KategorienDetailFragment"
        android:label="kategorienDetailFragment">
        <argument
            android:name="kategorieDetail"
            app:argType="de.syntaxinstitut.myapplication.data.KategorieDetailEnum" />
    </fragment>
    <fragment
        android:id="@+id/homescreenFragment"
        android:name="de.syntaxinstitut.myapplication.ui.fragmenteXml.HomescreenFragment"
        android:label="HomescreenFragment" />

</navigation>